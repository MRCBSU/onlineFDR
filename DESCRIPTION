Package: onlineFDR
Version: 1.3.7
Date: 2019-09-19
Title: Online FDR control
Authors@R: c(person(c("David", "S."), "Robertson", role = c("aut", "cre"),
                    email = "david.robertson@mrc-bsu.cam.ac.uk"),
             person("Aaditya", "Ramdas", role = "aut"),
